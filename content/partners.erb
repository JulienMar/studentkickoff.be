<header class="header-image">
  <div class="headline"></div>
</header>


<!-- Page Content -->
<div class="section1">
  <div class="container">

    <h1>Presenting Partners</h1>
    <div class="grid-presenting">
      <% item[:presentingpartners].each do |partner| %>
      <div class="grid-item-presenting partner">
        <a href="http://www.<%= partner[:url] %>">
          <img class="img-responsive" src="/assets/images/partners/<%= partner[:img] %>" alt="">
        </a>
      </div>
      <% end %>
    </div>
  </div>
</div>
<div class="section2">
  <div class="container">

    <h1>Partners</h1>
    <div class="grid">
      <% item[:partners].each do |partner| %>
        <div class="grid-item partner">
          <a href="http://www.<%= partner[:url] %>">
            <img class="img-responsive" src="/assets/images/partners/<%= partner[:img] %>" alt="">
          </a>
        </div>
      <% end %>
    </div>
  </div>
</div>

