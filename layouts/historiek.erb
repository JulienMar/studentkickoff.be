<div class="container">
  <div class="row">
      <div class="col-lg-12">
          <h1 class="page-header">Historiek van de Student Kick-Off</h1>
      </div>
  </div>
  <% item[:historiek].each do |hist| %>
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-9"><h3><%= hist[:jaar] %></h3></div>
    <img class="img-responsive col-md-3" src="<%= hist[:affiche] %>" alt="">
    <div class="col-md-9">
      <%= hist[:tekst] %>
      <h4>Line-Up</h4>
      <ul class="list-inline">
        <% hist[:lineup].each do |a| %>
          <li><%= a[:artist] %></li>
        <% end %>
      </ul>
    </div>
  </div>
  <hr>
  <% end %>
</div>


