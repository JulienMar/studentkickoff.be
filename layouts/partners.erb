
<!-- Page Content -->
<div class="container">

    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Presenting Partners</h1>
        </div>
    </div>
    <!-- /.row -->

    <!-- Projects Row -->
    <div class="row">
        <% item[:presentingpartners].each do |partner| %>
            <div class="col-md-3 portfolio-item">
                <a href="http://www.<%= partner[:url] %>">
                    <img class="img-responsive" src="/assets/images/partners/<%= partner[:img] %>" alt="">
                </a>
            </div>
        <% end %>
    </div>
    <!-- /.row -->

    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Partners</h1>
        </div>
    </div>

    <!-- Projects Row -->

    <div class="row">
        <% item[:partners].each do |partner| %>
            <div class="col-md-2 portfolio-item">
                <a href="http://www.<%= partner[:url] %>">
                    <img class="img-responsive" src="/assets/images/partners/<%= partner[:img] %>" alt="">
                </a>
            </div>
        <% end %>
    </div>
    <!-- /.row -->
</div>
